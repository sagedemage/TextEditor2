{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "eqeqeq": "error",
        "curly": "error",
        "quotes": [
            "error",
            "single"
        ],
        "no-unused-vars": "error",
        "prefer-const": "error"
    }
}